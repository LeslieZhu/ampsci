  Brueckner = false;
  n_min_core = 3;
  fitTo_cm;
  lambda_k;
}

//Spectrum
{
  number = 100;
  order = 7;
  r0_eps = 1.0e-9;
  rmax = 100.0;
  print = false;
  states = 90p;
}

